extends ../layout 
//-고객평가서 항목 수정//
block content 
    .container-lg
      .table-responsive
        .table-wrapper
          .table-title
            .row
              .col-sm-10
                h2  고객평가서 항목 수정
              .col-sm-1
                button.btn.btn-primary(type='button' value='add' title='add') 항목추가
              .col-sm-1
                button.btn.btn-dark(type='button' value='completion' title='completion') 수정완료
          table.table.table-bordered
            thead
              tr
                th 항목번호
                th 항목내용
                th Actions
            tbody
              tr
                td 01
                td 업무수행 평가
                td
                  button.btn.btn-danger(type='button' value='Edit' title='Edit') 수정
                  button.btn.btn-warning(type='button' value='Delete' title='Delete') 항목삭제   
              tr
                td 02
                td 커뮤니케이션 평가
                td
                  button.btn.btn-danger(type='button' value='Edit' title='Edit') 수정
                  button.btn.btn-warning(type='button' value='Delete' title='Delete') 항목삭제
              tr
                td 03
                td 기타 업무 평가
                td
                  button.btn.btn-danger(type='button' value='Edit' title='Edit') 수정
                  button.btn.btn-warning(type='button' value='Delete' title='Delete') 항목삭제
