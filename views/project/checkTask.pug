extends ../layout 

//- 프로젝트 수정
block content
  .container 
    form(action="#", method="post")
      h1.text-center-mt-5 프로젝트 진행 사항

      p(style="font-size: 24px") 착수 일자에 따른 진행 사항
          .progress
              div.progress-bar.progress-bar-striped(role="progressbar" style="width: 30%" aria-valuenow="30" aria-valuemin="0" aria-valuemax="100") 30%

      .card
        .card-body
          h5.card-title 직원 이름
          p.card-text 본인의 업무에 따른 진행 사항
          .progress
              div.progress-bar.progress-bar-striped.bg-success(role="progressbar" style="width: 70%" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100") 70%



